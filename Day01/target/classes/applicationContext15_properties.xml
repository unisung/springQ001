<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  
  <!--  collections injection -->
  <bean id="collectionBean" class="polymorphism15_collections_properties.CollectionBean">
     <property name="addresList">
     	<props>
     	    <prop key="고길동">서울시 강남구</prop>
     	    <prop key="일지매">서울시 마포구</prop>
     	    <prop key="임꺽정">수원시 영통구</prop>
     	</props>
     </property>
  </bean>        
</beans>
